{
  "name": "milife",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./node_modules ./.next ./.husky/_",
    "clean:cache": "rm -rf ./.next",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit --incremental",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "eslint": "eslint \"**/*.*{ts,tsx,js,jsx}\"",
    "eslint:fix": "eslint --fix \"**/*.*{ts,tsx,js,jsx}\"",
    "lint": "npm run eslint && npm run prettier && npm run type-check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prepare": "husky",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "update": "ncu -i",
    "update:check": "ncu",
    "update:all": "ncu -u && npm i",
    "predev": "node ./src/scripts/env-variables-checker.mjs",
    "prebuild": "node ./src/scripts/env-variables-checker.mjs",
    "generate:component": "hygen component-generator with-prompt",
    "generate:query": "hygen query-generator with-prompt"
  },
  "engines": {
    "node": "^20.9.0",
    "npm": "^10.2.3"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/base": "^5.0.0-beta.58",
    "@mui/icons-material": "^6.1.1",
    "@mui/lab": "^6.0.0-beta.10",
    "@mui/material": "^6.1.1",
    "@mui/system": "^6.1.1",
    "@mui/x-date-pickers": "^7.18.0",
    "@next/env": "^14.2.13",
    "@reduxjs/toolkit": "^2.2.7",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "formik": "^2.4.6",
    "lodash": "^4.17.21",
    "lottie-web": "^5.12.2",
    "next": "14.2.13",
    "next-redux-wrapper": "^8.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-transition-group": "^4.4.5",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss": "^3.4.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.13",
    "postcss": "^8",
    "typescript": "^5",
    "@cspell/eslint-plugin": "^8.14.4",
    "@types/json-stable-stringify": "^1.0.36",
    "@types/lodash": "^4.17.7",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^3.5.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.6",
    "hygen": "^6.2.11",
    "npm-check-updates": "^17.1.2",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "sass": "^1.79.1"
